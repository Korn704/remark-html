{
  "name": "remark-html",
  "version": "4.0.0",
  "description": "Compile Markdown to HTML with remark",
  "license": "MIT",
  "keywords": [
    "markdown",
    "html",
    "stringify",
    "compile",
    "remark"
  ],
  "dependencies": {
    "hast-util-sanitize": "^1.0.0",
    "hast-util-to-html": "^1.0.0",
    "mdast-util-to-hast": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/remark-html.git"
  },
  "bugs": "https://github.com/wooorm/remark-html/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "commonmark.json": "^0.25.0",
    "eslint": "^2.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^3.0.0",
    "jscs-jsdoc": "^2.0.0",
    "remark": "^5.0.0",
    "remark-cli": "^1.0.0",
    "remark-comment-config": "^4.0.0",
    "remark-github": "^5.0.0",
    "remark-lint": "^4.0.0",
    "remark-toc": "^3.0.0",
    "remark-usage": "^4.0.0",
    "remark-validate-links": "^4.0.0",
    "tape": "^4.0.0",
    "to-vfile": "^1.0.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s remarkHTML > remark-html.js",
    "build-mangle": "esmangle remark-html.js > remark-html.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test/index.js",
    "test-coverage": "istanbul cover test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
